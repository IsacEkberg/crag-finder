      <div class="panel panel-primary">
          <div class="panel-heading">
              <h3 class="panel-title">{{ climbingArea.name }}</h3>
          </div>
          <div class="panel-body">
              {{climbingArea.short_description}}
              {{#link-to "area" climbingArea}}LÃ¤nk{{/link-to}}
