    <div class="row">
        <div class="col-md-8 col-md-offset-2">
            <div class="brand">
                <h1>Crag finder</h1>
                <h3>Find crags close to you.</h3>
            </div>
        </div>
    </div>
  </div>
</div>

<div class="main main-raised">
    <div class="section section-basic">
        <div class="container">
                {{filter-crag filteredList=filteredList autoComplete=(action 'autoComplete') search=(action 'search')}}


